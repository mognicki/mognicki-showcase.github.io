ia/vue/calendar/MonthStrips.vue","name":"MonthStrips.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"}},{"path":"/varia/vue/calendar/RomanMonths.vue","name":"RomanMonths.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"}},{"path":"/varia/vue/calendar/Timeline.vue","name":"Timeline.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"},"content":"<template>\n  <div class=\"timeline-container flex flex-row w-100 h-100 items-stretch content-stretch\">\n    <div class=\"w-30 flex flex-row\">\n      <div class=\"w-25 h-100 flex content-center items-center justify-around\">\n        <ul class=\"list pa0 w-100 tc\">\n          <li\n            v-for=\"i in (edgeYears.max - (edgeYears.min - 1))\"\n            :key=\"i\"\n            :class=\"{ 'fw7 current-year': (edgeYears.max - (i - 1)) === year }\"\n            class=\"pointer year-item\"\n            @click=\"setYear(edgeYears.max - (i - 1))\"\n          >\n            {{ edgeYears.max - (i - 1) }}\n          </li>\n        </ul>\n      </div>\n      <div class=\"w-50 h-100 flex content-center items-center\">\n        <event-list :year-events=\"yearEvents\" />\n      </div>\n      <div class=\"w-25 h-100 flex flex-column pa0 items-stretch content-stretch\">\n        <roman-months v-show=\"event === null\" />\n      </div>\n    </div>\n    <div class=\"w-70 h-100\">\n      <div v-show=\"event === null\" class=\"timeline\" @wheel=\"handleWheel\">\n        <month-strips :year=\"year\" />\n        <year-events :events=\"yearEvents\" :year=\"year\" />\n      </div>\n      <event-content v-if=\"event\" :event=\"event\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapMutations } from 'vuex'\nimport MonthStrips from '~/components/experience/newer/MonthStrips'\nimport YearEvents from '~/components/experience/newer/YearEvents'\nimport EventList from '~/components/experience/newer/EventList'\nimport EventContent from '~/components/experience/newer/EventContent'\nimport RomanMonths from '~/components/experience/newer/RomanMonths'\nconst _ = require('lodash')\nexport default {\n  components: {\n    RomanMonths,\n    EventContent,\n    EventList,\n    MonthStrips,\n    YearEvents\n  },\n  props: {\n    events: {\n      type: Array,\n      required: true\n    },\n    rawEvents: {\n      type: Array,\n      required: true\n    },\n    event: {\n      type: Object,\n      default: null\n    }\n  },\n  computed: {\n    year () {\n      return this.$store.state.experience.year\n    },\n    yearEvents () {\n      return _.orderBy(this.rawEvents.filter((e) => {\n        return e.from.getFullYear() <= this.year && e.to.getFullYear() >= this.year\n      }), [(i) => {\n        return i.to - i.from\n      }], ['desc'])\n    },\n    edgeYears () {\n      const years = {\n        min: _.minBy(this.rawEvents, e => new Date(e.from), 'from').from.getFullYear(),\n        max: _.maxBy(this.rawEvents, e => new Date(e.to), 'to').to.getFullYear()\n      }\n      const now = new Date()\n\n      years.max = years.max > now.getFullYear() ? now.getFullYear() : years.max\n\n      return years\n    }\n  },\n  methods: {\n    handleWheel: _.debounce(function (e) {\n      this.doHandleWheel(e)\n    }, 1000),\n    doHandleWheel (e) {\n      if (e.deltaY < 0) {\n        if (this.year < this.edgeYears.max) {\n          this.setYear(this.year + 1)\n        }\n      } else if (this.year > this.edgeYears.min) {\n        this.setYear(this.year - 1)\n      }\n    },\n    ...mapMutations({\n      setYear: 'experience/setYear'\n    })\n  }\n}\n</script>\n\n<style lang=\"scss\">\n  .timeline-container {\n    height: 100vh;\n  }\n\n  .timeline {\n    position: relative;\n    height: 100vh;\n\n    .bar {\n      position: absolute;\n      top: 50%;\n      left: 0;\n      width: 100%;\n    }\n  }\n\n  .year-item {\n    font-size: 1.2rem;\n    color: #333;\n    opacity: 0.7;\n    transition: all 0.5s;\n\n    &.current-year {\n      font-s