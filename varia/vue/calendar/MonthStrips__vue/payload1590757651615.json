ia/vue/calendar/MonthStrips.vue","name":"MonthStrips.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"},"content":"<template>\n  <div class=\"w-100 h-100\">\n    <div class=\"month-strips  bg-light-gray\" :class=\"{ 'show-prev': showPrev, 'show-next': showNext, 'no-transition': !showPrev && !showNext }\">\n      <div v-if=\"showPrev\" class=\"prev  bg-light-gray\">\n        <div v-for=\"i in 12\" :key=\"i\" class=\"strip\" :style=\"stripOffset(i)\" />\n      </div>\n      <div class=\"current  bg-light-gray\">\n        <div v-for=\"i in 12\" :key=\"i\" class=\"strip\" :style=\"stripOffset(i)\" />\n      </div>\n      <div v-if=\"showNext\" class=\"next  bg-light-gray\">\n        <div v-for=\"i in 12\" :key=\"i\" class=\"strip\" :style=\"stripOffset(i)\" />\n      </div>\n    </div>\n    <div class=\"today\" :style=\"todayStyle\" />\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    year: {\n      type: Number,\n      required: true\n    }\n  },\n  data () {\n    return {\n      showNext: false,\n      showPrev: false\n    }\n  },\n  computed: {\n    todayStyle () {\n      const now = new Date()\n      const fixedTop = String(100 - (((this.dayOfYear(now, now.getFullYear())) / 365) * 100))\n      const top = fixedTop + '%'\n      let transform = null\n      if (this.year !== now.getFullYear()) {\n        if (this.year < now.getFullYear()) {\n          transform = 'translateY(-100vh)'\n        } else {\n          transform = 'translateY(100vh)'\n        }\n      } else {\n        transform = 'translateY(-50%)'\n      }\n\n      return {\n        top,\n        transform,\n        opacity: this.year === now.getFullYear() ? '1' : '0'\n      }\n    }\n  },\n  watch: {\n    year (value, oldValue) {\n      if (oldValue > value) {\n        this.showNext = true\n      } else {\n        this.showPrev = true\n      }\n\n      setTimeout(() => {\n        this.showNext = false\n        this.showPrev = false\n      }, 1000)\n    }\n  },\n  methods: {\n    dayOfYear (date, year) {\n      const first = new Date(this.year, 0, 1)\n      if (year !== undefined) {\n        first.setYear(year)\n      }\n      return Math.round(((date - first) / 1000 / 60 / 60 / 24) + 0.5)\n    },\n    stripOffset (i) {\n      const date = new Date(this.year, i, 1)\n      const lastDay = new Date(this.year, 12, 0)\n      const top = (this.dayOfYear(date) / this.dayOfYear(lastDay)) * 100\n      return {\n        top: top + '%'\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n  .month-strips {\n    position: relative;\n    width: 100%;\n    height: 100%;\n\n    &:not(.no-transition) {\n      transition: transform 0.5s;\n    }\n\n    &.show-prev {\n      transform: translateY(100%);\n    }\n\n    &.show-next {\n      transform: translateY(-100%);\n    }\n\n    .prev, .next {\n      position: absolute;\n      top: -100%;\n      height: 100%;\n      left: 0;\n      width: 100%;\n    }\n\n    .prev {\n      top: -100%;\n    }\n\n    .next {\n      top: 100%;\n    }\n\n    .strip {\n      position: absolute;\n      height: 1px;\n      width: 100%;\n      border-top: 1px solid rgba(0,0,0,.1);\n    }\n  }\n\n  .today {\n    position: absolute;\n    left: 0;\n    width: 100%;\n    height: 5px;\n    background: rgba(0,0,0,0.2);\n\n    transition: all 0.5s;\n  }\n</style>\n"},{"path":"/varia/vue/calendar/RomanMonths.vue","name":"RomanMonths.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"}},{"path":"/varia/vue/calendar/Timeline.vue","name":"Timeline.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"}},{"path":"/varia/vue/calendar/YearEvents.vue","name":"YearEvents.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"}}],"exclude":[{}],"yaml":{}},{"path":"/varia/vue/directory-tree","name":"directory-tree","type":"dir","children":[{"path":"/varia/vue/directory-tree/tree","name":"tree","type":"dir","children":[{"path":"/varia/vue/directo