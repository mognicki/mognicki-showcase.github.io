ate>\n\n<script>\nexport default {\n  props: {\n    year: {\n      type: Number,\n      required: true\n    }\n  },\n  data () {\n    return {\n      showNext: false,\n      showPrev: false\n    }\n  },\n  computed: {\n    todayStyle () {\n      const now = new Date()\n      const fixedTop = String(100 - (((this.dayOfYear(now, now.getFullYear())) / 365) * 100))\n      const top = fixedTop + '%'\n      let transform = null\n      if (this.year !== now.getFullYear()) {\n        if (this.year < now.getFullYear()) {\n          transform = 'translateY(-100vh)'\n        } else {\n          transform = 'translateY(100vh)'\n        }\n      } else {\n        transform = 'translateY(-50%)'\n      }\n\n      return {\n        top,\n        transform,\n        opacity: this.year === now.getFullYear() ? '1' : '0'\n      }\n    }\n  },\n  watch: {\n    year (value, oldValue) {\n      if (oldValue > value) {\n        this.showNext = true\n      } else {\n        this.showPrev = true\n      }\n\n      setTimeout(() => {\n        this.showNext = false\n        this.showPrev = false\n      }, 1000)\n    }\n  },\n  methods: {\n    dayOfYear (date, year) {\n      const first = new Date(this.year, 0, 1)\n      if (year !== undefined) {\n        first.setYear(year)\n      }\n      return Math.round(((date - first) / 1000 / 60 / 60 / 24) + 0.5)\n    },\n    stripOffset (i) {\n      const date = new Date(this.year, i, 1)\n      const lastDay = new Date(this.year, 12, 0)\n      const top = (this.dayOfYear(date) / this.dayOfYear(lastDay)) * 100\n      return {\n        top: top + '%'\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n  .month-strips {\n    position: relative;\n    width: 100%;\n    height: 100%;\n\n    &:not(.no-transition) {\n      transition: transform 0.5s;\n    }\n\n    &.show-prev {\n      transform: translateY(100%);\n    }\n\n    &.show-next {\n      transform: translateY(-100%);\n    }\n\n    .prev, .next {\n      position: absolute;\n      top: -100%;\n  