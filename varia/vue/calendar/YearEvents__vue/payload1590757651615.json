ia/vue/calendar/MonthStrips.vue","name":"MonthStrips.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"}},{"path":"/varia/vue/calendar/RomanMonths.vue","name":"RomanMonths.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"}},{"path":"/varia/vue/calendar/Timeline.vue","name":"Timeline.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"}},{"path":"/varia/vue/calendar/YearEvents.vue","name":"YearEvents.vue","type":"file","exclude":[{}],"stacks":{"language":"vue","icon":"logos:vue"},"content":"<template>\n  <transition-group\n    name=\"list\"\n    tag=\"div\"\n    class=\"year-events ml4 pa4\"\n  >\n    <template v-for=\"(event, i) in fEvents\">\n      <div\n        :id=\"'event-' + event.id\"\n        :key=\"event.id\"\n        class=\"event-box multiple-months pointer\"\n        :style=\"eventBoxStyle(event, i)\"\n        :class=\"eventStyle(event, i)\"\n        @mouseenter=\"highlightEvent(event.id)\"\n        @mouseleave=\"concealEvent\"\n      >\n      </div>\n    </template>\n  </transition-group>\n</template>\n\n<script>\nimport { mapMutations } from 'vuex'\n\nconst _ = require('lodash')\nexport default {\n  props: {\n    events: {\n      type: Array,\n      required: true\n    },\n    year: {\n      type: Number,\n      required: true\n    }\n  },\n  data () {\n    return {\n      fEvents: []\n    }\n  },\n  computed: {\n    eventHighlightId () {\n      return this.$store.state.experience.highlightId\n    }\n  },\n  watch: {\n    events (value) {\n      // let newIds = []\n      const valueIds = value.map(i => i.id)\n      const fIds = this.fEvents.map(i => i.id)\n      const newIds = _.difference(valueIds, fIds)\n      // const oldIds = _.difference(fIds, valueIds)\n      const freshIds = []\n      for (const i in fIds) {\n        if (valueIds.includes(fIds[i])) {\n          freshIds.push(fIds[i])\n        } else {\n          const newId = newIds.shift()\n          freshIds.push(newId)\n        }\n      }\n\n      let fEvents = freshIds\n\n      if (newIds.length > 0) {\n        fEvents = fEvents.concat(newIds)\n      }\n\n      fEvents = (fEvents.map((i) => {\n        let candidate = value.find(e => e.id === i)\n        if (candidate !== undefined) {\n          return candidate\n        }\n        candidate = this.fEvents.find(e => e.id === i)\n        if (candidate !== undefined) {\n          return candidate\n        }\n        return null\n      }))\n\n      this.fEvents = fEvents.filter(e => e !== null && e !== undefined)\n    }\n  },\n  mounted () {\n    this.fEvents = this.events\n\n    this.$bus.$on('highlight-event', (id) => {\n      this.eventHighlightId = id\n    })\n  },\n  methods: {\n    eventStyle (event, i) {\n      const delay = 'delay-' + String(Math.round(1 + Math.random() * 9))\n      let styleTxt = ''\n      if (event.from.getFullYear() === this.year) {\n        styleTxt += 'dot-bottom '\n      }\n      if (event.to.getFullYear() === this.year && !event.ongoing) {\n        styleTxt += 'dot-top '\n      }\n      styleTxt += delay\n\n      if (event.id === this.eventHighlightId) {\n        styleTxt += ' highlight'\n      }\n\n      return styleTxt\n    },\n    randomDelay () {\n      const delay = String(Math.round(1 + Math.random() * 9))\n      return 'delay-' + delay\n    },\n    dayOfYear (date) {\n      const first = new Date(this.year, 0, 1)\n      return Math.round(((date - first) / 1000 / 60 / 60 / 24) + 0.5)\n    },\n    eventBoxStyle (event, i) {\n      let top = Math.floor(100 - ((this.dayOfYear(event.showTo) / 365) * 100))\n      let bottom = Math.ceil(100 - ((this.dayOfYear(event.showFrom) / 365) * 100))\n      top = top < 0 ? 0 : top\n      bottom = bottom > 100 ? 100 : bottom\n      const height = Math.abs(parseInt(bottom - top))\n      const width = 90 / (event.overlaps + 1)\n      const left = (event.position / (event.overlaps + 1)) * 100\n      const opacity = this.eventHighlightId === null ? null : (event.id === this.eventHighlightId ? 1 : 0.2)\n      return {\n        top: top + '%',\n        height: height + '%',\n        left: String(left) + '%',\n        width: width + '%',\n        // backgroundColor: _.first(event.skills) !== undefined ? '#' + _.first(event.skills).background : '#fff'\n        backgroundColor: event.color,\n        opacity\n      }\n    },\n    ...mapMutations({\n      highlightEvent: 'experience/highlight',\n      concealEvent: 'experience/conceal'\n    })\n  }\n}\n</script>\n\n<style lang=\"scss\">\n  .year-events {\n    position: absolute;\n    left: 0;\n    top: 0;\n    width: 100%;\n    height: 100%;\n  }\n\n  /* ----------------------------------------------\n * Generated by Animista on 2020-5-16 3:55:6\n * Licensed under FreeBSD License.\n * See http://animista.net/license for more info.\n * w: http://animista.net, t: @cssanimista\n * ---------------------------------------------- */\n\n  /**\n   * ----------------------------------------\n   * animation vibrate-1\n   * ----------------------------------------\n   */\n  @keyframes vibrate-1 {\n    0% {\n      transform: translate(0);\n    }\n    20% {\n      transform: translate(-2px, 2px);\n    }\n    40% {\n      transform: translate(-2px, -2px);\n    }\n    60% {\n      transform: translate(2px, 2px);\n    }\n    80% {\n      transform: translate(2px, -2px);\n    }\n    100% {\n      transform: translate(0);\n    }\n  }\n\n  .event-box {\n    position: absolute;\n    transition: all 0.5s;\n    padding: 0;\n    max-width: 20px;\n\n    background: transparent;\n    border-radius: 10px;\n\n    &:not(.highlight) {\n      animation: none;\n    }\n\n    &.highlight {\n      animation: vibrate-1 1s linear infinite both;\n    }\n\n    .event-name {\n      display: none;\n    }\n\n    &:not(.dot-top) {\n      border-top-right-radius: 0 !important;\n      border-top-left-radius: 0 !important;\n    }\n    &:not(.dot-bottom) {\n      border-bottom-right-radius: 0 !important;\n      border-b